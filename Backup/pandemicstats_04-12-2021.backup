-- TOC entry 233 (class 1259 OID 34055)
-- Name: vw_select1; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW public.vw_select1 AS
 SELECT p.codpac,
    u.nomusu
   FROM (((public.paciente p
     JOIN public.usuario u ON ((u.cpfusu = p.cpfusu_cpfusu)))
     JOIN public.monitoramento_paciente mp ON ((mp.codpac_codpac = p.codpac)))
     JOIN public.sintoma s ON ((mp.codsin_codsin = s.codsin)))
  WHERE (((s.nomsin)::text ~~* '%tosse%'::text) AND ((date_part('year'::text, CURRENT_TIMESTAMP) - date_part('year'::text, u.datnasusu)) >= (40)::double precision) AND ((date_part('year'::text, CURRENT_TIMESTAMP) - date_part('year'::text, u.datnasusu)) <= (50)::double precision))
  ORDER BY p.codpac DESC;


ALTER TABLE public.vw_select1 OWNER TO postgres;

--
-- TOC entry 234 (class 1259 OID 34060)
-- Name: vw_select2; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW public.vw_select2 AS
 SELECT DISTINCT u.nomusu,
    c.nomcid
   FROM ((((public.paciente p
     JOIN public.usuario u ON ((u.cpfusu = p.cpfusu_cpfusu)))
     JOIN public.cidade c ON ((c.codcid = u.codcid_codcid)))
     JOIN public.teste_covid tc ON ((tc.codpac_codpac = p.codpac)))
     JOIN public.monitoramento_paciente mp ON ((mp.codpac_codpac = p.codpac)))
  WHERE ((tc.covpactes = 'N'::bpchar) AND (mp.codsin_codsin IS NOT NULL) AND (mp.intsin <> 'S'::bpchar) AND (u.sexusu = 'M'::bpchar) AND ((c.nomcid)::text = ANY ((ARRAY['Maravilha'::character varying, 'Descanso'::character varying, 'Pinhalzinho'::character varying, 'Chapecó'::character varying, 'Itapiranga'::character varying])::text[])))
  ORDER BY c.nomcid DESC, u.nomusu;


