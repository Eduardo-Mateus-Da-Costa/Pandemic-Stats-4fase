CREATE OR REPLACE VIEW public.vw_select3 AS
 SELECT c.codcid,
    c.nomcid,
    count(DISTINCT mp.codpac_codpac) AS conta
   FROM (((public.cidade c
     LEFT JOIN public.usuario u ON ((c.codcid = u.codcid_codcid)))
     LEFT JOIN public.paciente p ON ((p.cpfusu_cpfusu = u.cpfusu)))
     LEFT JOIN public.monitoramento_paciente mp ON ((mp.codpac_codpac = p.codpac)))
  GROUP BY c.codcid
  ORDER BY (count(DISTINCT mp.codpac_codpac));


