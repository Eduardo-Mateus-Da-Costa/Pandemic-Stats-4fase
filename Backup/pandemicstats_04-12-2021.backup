    count(*) AS casos
   FROM ((public.paciente p
     JOIN public.teste_covid tc ON ((tc.codpac_codpac = p.codpac)))
     JOIN public.usuario u ON ((u.cpfusu = p.cpfusu_cpfusu)))
  WHERE ((tc.covpactes = 'P'::bpchar) AND (((date_part('day'::text, tc.dattes))::integer % 2) = 0))
  GROUP BY (to_char(age((u.datnasusu)::timestamp with time zone), 'YY'::text))
  ORDER BY (count(*)) DESC;


